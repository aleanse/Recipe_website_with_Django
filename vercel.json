{
    "builds": [{
        "src": "project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    
    "env": {
        "MYSQLCLIENT_CFLAGS": "-I/usr/include/mysql",
        "MYSQLCLIENT_LDFLAGS": "-L/usr/lib/mysql -lmysqlclient"
    },
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/wsgi.py"
        }
    ]
}